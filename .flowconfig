[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.mjs
module.file_ext=.json
module.file_ext=.vert
module.file_ext=.frag
module.file_ext=.scss
module.name_mapper.extension='vert' -> '<PROJECT_ROOT>/common/utils/RawTextModule.js'
module.name_mapper.extension='frag' -> '<PROJECT_ROOT>/common/utils/RawTextModule.js'
module.name_mapper='models\(.*\)\.json$' -> '<PROJECT_ROOT>/common/utils/MeshJSON.js'

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
